{
  "name": "scrum-calendar",
  "version": "1.0.0",
  "description": "Scrum Calendar web application for visualizing Azure DevOps PBIs",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch src/server --ext ts --exec \"ts-node -P tsconfig.server.json src/server/index.ts\"",
    "dev:client": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "tsc -p tsconfig.client.json && vite build",
    "start": "node dist/server/index.js",
    "test": "jest",
    "lint": "prettier --check .",
    "lint:check": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint src/client --ext .ts,.tsx",
    "lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint src/client --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "changelog": "node scripts/add-changelog.js"
  },
  "keywords": [
    "scrum",
    "calendar",
    "azure-devops"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/arm-costmanagement": "^1.0.0-beta.1",
    "@azure/arm-resources": "^7.0.0",
    "@azure/arm-resources-subscriptions": "^2.1.0",
    "@azure/arm-subscriptions": "^6.0.0",
    "@azure/identity": "^4.13.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@types/express-session": "^1.18.2",
    "@types/passport": "^1.0.17",
    "azure-devops-node-api": "^12.5.0",
    "connect-ensure-login": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "react-error-boundary": "^6.1.1",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "session-file-store": "^1.5.0",
    "uuid": "^9.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/passport-azure-ad": "^4.3.6",
    "@types/react": "^18.2.48",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "date-fns": "^2.30.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-big-calendar": "^1.11.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
